  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "text-summary",
      "lcov",
      "cobertura",
      "html",
      "text"
    ],
    "testMatch": [
      "**/*.test.js"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "env-cmd -f .env.development react-scripts start",
    "stage": "env-cmd -f .env.qa react-scripts start",
    "build:development": "env-cmd -f .env.development react-scripts build",
    "build:qa": "env-cmd -f .env.qa react-scripts build",
    "test": "react-scripts test --watchAll --testMatch **/src/**/**/**/underwritter.test.jsx",
    "coverage": "react-scripts test --env=jsdom --watchAll --coverage --testMatch **/src/**/**/**/*.test.jsx",
    "eject": "react-scripts eject",
    "lint": "eslint --cache --cache-location \"node_modules/.cache/.eslintcache\" --fix --max-warnings 0 \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:js": "eslint src/**/*.js --cache --fix",
    "lint:jsx": "eslint src/**/*.jsx --cache --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
